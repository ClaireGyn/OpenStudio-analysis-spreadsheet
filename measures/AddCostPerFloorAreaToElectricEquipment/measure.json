{
  "classname": "AddCostPerFloorAreaToElectricEquipment",
  "path": "./measures/AddCostPerFloorAreaToElectricEquipment",
  "name": "Add Cost per Floor Area to Electric Equipment",
  "measure_type": "RubyMeasure",
  "arguments": [
    {
      "local_variable": "equip_def",
      "variable_type": "Choice",
      "name": "equip_def",
      "display_name": "Choose a Watts per Area Electric Equipment Definition to Add Costs to.",
      "choices": [

      ]
    },
    {
      "local_variable": "remove_costs",
      "variable_type": "Bool",
      "name": "remove_costs",
      "display_name": "Remove Existing Costs?",
      "default_value": true
    },
    {
      "local_variable": "material_cost_ip",
      "variable_type": "Double",
      "name": "material_cost_ip",
      "display_name": "Material and Installation Costs for Definition per Floor Area ($/ft^2).",
      "default_value": 0.0
    },
    {
      "local_variable": "demolition_cost_ip",
      "variable_type": "Double",
      "name": "demolition_cost_ip",
      "display_name": "Demolition Costs for Definition per Floor Area ($/ft^2).",
      "default_value": 0.0
    },
    {
      "local_variable": "years_until_costs_start",
      "variable_type": "Integer",
      "name": "years_until_costs_start",
      "display_name": "Years Until Costs Start (whole years).",
      "default_value": 0
    },
    {
      "local_variable": "demo_cost_initial_const",
      "variable_type": "Bool",
      "name": "demo_cost_initial_const",
      "display_name": "Demolition Costs Occur During Initial Definition?",
      "default_value": false
    },
    {
      "local_variable": "expected_life",
      "variable_type": "Integer",
      "name": "expected_life",
      "display_name": "Expected Life (whole years).",
      "default_value": 20
    },
    {
      "local_variable": "om_cost_ip",
      "variable_type": "Double",
      "name": "om_cost_ip",
      "display_name": "O & M Costs for Definition per Floor Area ($/ft^2).",
      "default_value": 0.0
    },
    {
      "local_variable": "om_frequency",
      "variable_type": "Integer",
      "name": "om_frequency",
      "display_name": "O & M Frequency (whole years).",
      "default_value": 1
    }
  ]
}